{
  "name": "3dime-angular",
  "version": "3.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "serve:ng": "ng serve --configuration=local",
    "serve:functions": "firebase emulators:start --only functions",
    "build:functions": "npm run build --prefix functions -- --watch",
    "dev": "concurrently -k -n \"ANGULAR,FIREBASE,BUILD\" -c \"cyan,green,yellow\" \"npm run serve:ng\" \"npm run serve:functions\" \"npm run build:functions\"",
    "vercel-build": "node scripts/set-env.js && ng build"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/service-worker": "^21.1.3",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@notionhq/client": "^5.9.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "cal-heatmap": "^4.2.4",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/localize": "^21.1.3",
    "@types/jasmine": "~6.0.0",
    "@types/node": "^25.2.1",
    "angular-eslint": "21.2.0",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.0",
    "jasmine-core": "~6.0.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.54.0"
  }
}