{
  "name": "3dime-angular",
  "version": "2.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "serve:ng": "ng serve --configuration=local",
    "serve:functions": "firebase emulators:start --only functions",
    "build:functions": "npm run build --prefix functions -- --watch",
    "dev": "concurrently -k -n \"ANGULAR,FIREBASE,BUILD\" -c \"cyan,green,yellow\" \"npm run serve:ng\" \"npm run serve:functions\" \"npm run build:functions\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.10",
    "@angular/cdk": "^20.2.12",
    "@angular/common": "^20.3.10",
    "@angular/compiler": "^20.3.10",
    "@angular/core": "^20.3.10",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.3.10",
    "@angular/platform-browser": "^20.3.10",
    "@angular/router": "^20.3.10",
    "@angular/service-worker": "^20.3.10",
    "@fullcalendar/angular": "^6.1.19",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@notionhq/client": "^5.3.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "cal-heatmap": "^4.2.4",
    "cors": "^2.8.5",
    "firebase": "^12.5.0",
    "ical.js": "^2.2.1",
    "pdfjs-dist": "^5.4.394",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "vite-plugin-ignore-dynamic-imports": "^1.0.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.10",
    "@angular/localize": "^20.3.10",
    "@types/jasmine": "~5.1.12",
    "@types/node": "^24.10.0",
    "angular-eslint": "20.5.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.46.3"
  }
}
